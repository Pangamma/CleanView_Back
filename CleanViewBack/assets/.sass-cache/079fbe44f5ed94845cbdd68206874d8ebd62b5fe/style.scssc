  fill: $grayCustom;
  cursor: pointer;
  @include transition(all .3s ease);
}
.b-day-event-list__check-mark:hover{
  fill: $gold;
}
.b-day-event-list-item__flag, .b-day-event-list-item__delete{
  margin-left: 10px;
}
.b-day-event-list-item__flag{
  background-color: white;
  color: red;
  border: 2px solid red;
  width: 70px;
  @include transition(all .3s ease);
}
.b-day-event-list-item__delete{
  background-color: white;
  color: $plump;
  border: 2px solid $plump;
  width: 70px;
  @include transition(all .3s ease);
}
.b-day-event-list-item__text{
  font-size: 16px; 
  margin: 10px;  
  float: left;
}
.b-cal-navlist__logout-link, .b-cal-navlist__logout-link:hover, .b-cal-navlist__logout-link:active{
  color: white;
  display: block;
  text-decoration: none;
}
.b-button-list, .b-cal-navlist{
  list-style-type: none;
  display: inline-block;
  float: right;
}
.b-button-list__button, .b-cal-navlist__item{
  display: inline-block;
}
.b-button-list{
  border-right: 2px solid $plump;
}
.b-button-list__button{
  border-top: 2px solid $plump;
  border-bottom: 2px solid $plump;
  border-left: 2px solid $plump;
  padding-top: 3px;
  padding-bottom: 3px;
  width: 70px;
  color: $plump;
  text-align: center;
}
.b-button-list__button--active{
  background-color: $plump;
  .b-button-list__link{
     color: white;
  }
}
.b-button-list__link{
  color: $plump;
  display: block;
}
.b-button-list__link, .b-button-list__link:hover, .b-button-list__link:active{
  text-decoration: none;
}

.b-cal-navlist{
  margin-right: 20px;
}
.b-day-dash, .b-main-dash, .b-conf-dash{
  background-color: $gray-bknd;
}
.b-day-dash{
  @include column(2);
}
.b-main-dash{
  @include column(7);
}
.b-conf-dash{
  @include column(3);
}
.b-calendar-dash{
  @include column(9);
}
.b-calendar-dash__container{
  background-color: white;
  margin-top: 5px;
  margin-bottom: 5px; 
}
.b-day-dash__container{
  background-color: white; 
  margin-top: 5px;
  margin-right: 10px;
  margin-bottom: 5px;
}
.b-conf-dash__container{
  background-color: white; 
  margin-top: 5px;
  margin-left: 10px;
  margin-bottom: 5px;
}
.b-main-dash__container{
  //background-color: white; 
  margin-top: 5px;
  margin-bottom: 5px;
}
.b-day-dash__day--active{
  background-color: $plump;
  color: white;
}
.b-day-dash__up, .b-day-dash__down{
  color: rgba(91,59,112,0.1);
  @include transition(all .3s ease);
  cursor: pointer;
}
.b-day-dash__up:hover, .b-day-dash__down:hover{
  background-color: $plump;
  @include transition(all .3s ease);
}
.b-day-dash__day-name, .b-day-dash__month-year, .b-day-dash__day-num, .b-day-dash__year, .b-day-dash__time{
  text-align: center;
}
.b-day-dash__day-name{
  font-size: 25px;
  line-height: 25px;
}
.b-day-dash__month-year{
  font-size: 18px;
}
.b-day-dash__day-num{
  font-size: 65px;
  line-height: 65px;
}
.b-day-dash__time{
  font-size: 20px;
  line-height: 20px;
  margin-top: 5px;
}

.b-day-dash__up-icon{
  
}
/*------- END Calendar day page ---------*/

/*------- Calendar Conf Dash ------------*/
.b-conf-dash__title{
  font-size: 16px; 
  text-align: center; 
  line-height: 16px; 
  margin: 0; 
  padding-top: 20px; 
  padding-bottom: 10px;
}
.b-dash-class-list, .b-dash-group-list{
  list-style-type: none; 
  border-top: 2px solid #e6e6e6; 
  margin-left: 10px; 
  margin-right: 10px;
  padding-top: 10px;
}
.b-dash-class-list__item, .b-dash-group-list__item{
  padding-bottom: 10px;
}
.b-dash-class-list__link, .b-dash-group-list__link{
  display: block; 
  border-bottom: 2px solid #e6e6e6; 
  padding-bottom: 10px;
  color: black;
  @include transition(all .3s ease);
}
.b-dash-class-list__link:hover, .b-dash-group-list__link:hover, .b-dash-class-list__link:active, .b-dash-group-list__link:active{
  text-decoration: none;
  cursor: pointer;
}
.b-dash-class-list__link:hover, .b-dash-class-list__link:active{
  border-color: $plump;
  color: $plump;
}
.b-dash-group-list__item{
  text-align: center;
}
.b-dash-class-list__class{
  margin-left: 10px; 
  background-color: $plump; 
  padding: 5px; 
  color: white;
}
.b-dash-class-list__name{
  margin-left: 10px;
}
.b-conf-dash__button{
  margin-top: 10px; 
  margin-bottom: 20px; 
  margin-left: 10px; 
  margin-right: 10px; 
  border: 2px solid $plump; 
  color: $plump; 
  text-align: center; 
  background-color: white; 
  margin-left: auto; 
  margin-right: auto; 
  display: block; 
  float: none; 
  padding-left: 10px; 
  padding-right: 10px;
  @include transition(all .3s ease);
}
.b-conf-dash__button:hover, .b-conf-dash__button:active{
  background-color: $plump;
  color: white !important;
}
/*------- END Calendar Conf Dash --------*/
.g-container{
  @extend .container;
}
/* Footer */
	footer{@extend .g-span12;}
/* END Footer */


/* Responsiveness */
@media (min-width: 768px) and (max-width: 1033px) { 
  .b-signup-form, .b-footer__container--signup-form{
    @include column(8);
    @include push(2);
    @include pull(2);
  }
  .b-footer__container--signup-form{
    margin-top: 20px;
  }  
}
@media (min-width: 585px) and (max-width: 767px) { 
  .b-signup-form, .b-footer__container--signup-form {
    @include column(10);
    @include push(1);
    @include pull(1);
  }
  .b-footer__container--signup-form{
    margin-top: 20px;
  }   
}
@media (max-width: 584px){
  .b-body--signup{
    padding-left: 20px;
    padding-right: 20px;
  }
  .b-signup-form, .b-signup-form__create-account, .b-signup-form__social, .b-footer__container--signup-form{
    @include column(12);
  }
  .b-signup-form__create-account, .b-signup-form__social, .b-footer__container--signup-form{
    text-align: center;
  }
  .b-signup-form__input-group, .b-signup-form__title{
    margin-left: 0;
  }
  .b-signup-form__button{
    margin: auto;
    margin-top: 20px;
  }
  .b-footer__container--signup-form{
    margin-top: 20px;
  } 
}  

/* footer */
.b-footer{
  margin-left: 20px;
  margin-right: 20px;
}
.b-footer__container{
  max-width: 1000px;
}
.b-footer__container--auto{
  margin: auto;
}

/* SVG icons */
.b-calendar-icon{
  color: $plump;
}
